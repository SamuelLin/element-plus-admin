{
  "name": "element-plus-test",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "lint": "eslint -c .eslintrc.js --fix --ext .js,.ts,.vue .",
